{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Launch Chrome against localhost, with sourcemaps",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "Attach to Chrome, with sourcemaps",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}"
    },
    {
      "name": "BoardReader", 
      "type": "chrome",
      "request": "launch",
      "file": "C:\\Users\\chris\\OneDrive\\Chris C Jamison, Inc\\Projects\\Apps\\BoardReader\\Source Code\\background.html",
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}"
    }
  ]
}